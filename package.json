{
  "name": "MerakiSwapper",
  "version": "1.0.0",
  "private": true,
  "description": "A Cisco Meraki tool to swap devices",
  "author": "dexterlabora",
  "scripts": {
    "build": "nuxt build",
    "dev": "cross-env NODE_ENV=development nodemon --watch api --exec nuxt",
    "devOld": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "docker": "docker build . -t app && docker run -d -p 8080:8080 app",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "start": "cross-env NODE_ENV=production node server/index.js"
  },
  "dependencies": {
    "@bazzite/nuxt-netlify": "^0.1.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/vuetify": "0.5.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.19",
    "meraki": "^1.1.0",
    "net": "^1.0.2",
    "nuxt": "^2.8.1",
    "request": "^2.88.0",
    "tls": "^0.0.1",
    "vue-cc-quaggajs": "^2.2.2",
    "vue-quaggajs": "^1.0.10"
  },
  "devDependencies": {
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "nodemon": "^1.18.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-docker": "^0.2.2"
  }
}
